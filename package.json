{
  "name": "p5-resources",
  "displayName": "p5 Resources",
  "description": "Resources that can be used with p5zs and p5ts projects.",
  "version": "2.3.0",
  "publisher": "zsakowitz",
  "icon": "favicon.png",
  "repository": {
    "url": "https://github.com/zsakowitz/p5-resources"
  },
  "engines": {
    "vscode": "^1.60.0"
  },
  "author": {
    "email": "zsakowitz@gmail.com",
    "name": "Zachary Sakowitz",
    "url": "https://github.com/zsakowitz"
  },
  "bugs": "https://github.com/zsakowitz/p5-resources/issues",
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:p5zs.open-glitch",
    "onCommand:p5zs.remove-glitch",
    "onCommand:p5ts.open-glitch",
    "onCommand:p5ts.remove-glitch",
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "index.js",
  "extensionPack": [
    "ritwickdey.liveserver",
    "auchenberg.vscode-browser-preview"
  ],
  "contributes": {
    "commands": [
      {
        "command": "p5zs.open-glitch",
        "title": "P5ZS: Open the p5zs starter Glitch project. (Uses JavaScript)"
      },
      {
        "command": "p5zs.remix-glitch",
        "title": "P5ZS: Remix the p5zs starter Glitch project. (Uses JavaScript)"
      },
      {
        "command": "p5ts.open-glitch",
        "title": "P5TS: Open the p5ts starter Glitch project. (Uses TypeScript)"
      },
      {
        "command": "p5ts.remix-glitch",
        "title": "P5TS: Remix the p5ts starter Glitch project. (Uses TypeScript)"
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/typescript.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/javascript.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/typescript.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^8.10.25",
    "@types/vscode": "^1.51.0"
  }
}
